{% extends 'base.html.twig' %}
{% block body %}
    <h1>Modifier site :</h1>
    <div class="row">
        <div class="col-8 offset-2">
            {{ form(form) }}
        </div>
    </div>

    {# Affichage des Flashes #}
    {% for label, messages in app.flashes %}
        {% for message in messages %}
            <div>{{ label }} : {{ message }}</div>
        {% endfor %}
    {% endfor %}

{% endblock %}
